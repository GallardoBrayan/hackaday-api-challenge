{
  "name": "supplyframe-challenge",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "scss": "node-sass --watch scss -o css",
    "build-css": "node-sass --include-path scss scss/main.scss public/stylesheets/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "test": "nightwatch"
  },
  "dependencies": {
    "chromedriver": "^74.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "^2.6.1",
    "express": "~4.16.0",
    "hjs": "~0.0.6",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "nightwatch": "^1.0.19",
    "node-sass": "^4.12.0",
    "request": "^2.88.0"
  }
}
